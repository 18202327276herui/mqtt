namespace InstantMessaging.Core.Models.Public.Mqtt
{
    public class FileDescription
    {
        /// <summary>
        ///  oss 上的 key
        /// </summary>
        public string Key { get; set; }
        /// <summary>
        /// 文件长度
        /// </summary>
        public long Length { get; set; }
        /// <summary>
        /// 文件名
        /// 1.发送者消息存储的应是文件的全路径
        /// 2.接收者消息存储的就是文件名
        /// </summary>
        public string FileName { get; set; }
    }
}
